{
    "asi": true,
    "bitwise": false,
    "camelcase": false,
    "curly": false,
    "eqeqeq": false,
    "forin": true,
    "freeze": true,
    "futurehostile": true,
    "globals": { "Promise": true, "it": false, "describe": false, "before": false, "after": false },
    "latedef": "nofunc",
    "laxbreak": true,
    "maxcomplexity": false,
    "maxdepth": 4,
    "maxparams": 5,
    "maxstatements": false,
    "newcap": true,
    "noarg": true,
    "nocomma": true,
    "node": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": false,
    "regexp": true,
    "singleGroups": false,
    "strict": false,
    "sub": true,
    "trailing": true,
    "undef": true,
    "unused": "vars",
    "varstmt": false
}
