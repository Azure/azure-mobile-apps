<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>An ASP.NET Core based framework for synchronizing structured data between a database and a client application.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IconUrl>http://go.microsoft.com/fwlink/?LinkID=288890</IconUrl>
    <LangVersion>10.0</LangVersion>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageId>Microsoft.AspNetCore.Datasync</PackageId>
    <PackageTags>microsoft;aspnetcore;cloud;datasync;webapi</PackageTags>
    <ProductName>Microsoft.AspNetCore.Datasync</ProductName>
    <ProjectUrl>https://github.com/Azure/azure-mobile-apps</ProjectUrl>
    <RepositoryUrl>https://github.com/Azure/azure-mobile-apps</RepositoryUrl>
    <RootNamespace>Microsoft.AspNetCore.Datasync</RootNamespace>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <Import Project="..\..\Shared.Build.props" />

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.JsonPatch" Version="6.0.13" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="6.0.13" />
    <PackageReference Include="Microsoft.AspNetCore.OData" Version="8.2.3" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Microsoft.AspNetCore.Datasync.Abstractions\Microsoft.AspNetCore.Datasync.Abstractions.csproj" />
  </ItemGroup>
</Project>
