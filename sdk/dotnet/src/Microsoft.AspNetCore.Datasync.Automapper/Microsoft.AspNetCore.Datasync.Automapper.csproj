<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>An Automapper based repository for use with the ASP.NET Core Datasync framework.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IconUrl>http://go.microsoft.com/fwlink/?LinkID=288890</IconUrl>
    <LangVersion>10.0</LangVersion>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageId>Microsoft.AspNetCore.Datasync.Automapper</PackageId>
    <PackageTags>microsoft;aspnetcore;cloud;datasync;webapi</PackageTags>
    <ProductName>Microsoft.AspNetCore.Datasync.Automapper</ProductName>
    <ProjectUrl>https://github.com/Azure/azure-mobile-apps</ProjectUrl>
    <RepositoryUrl>https://github.com/Azure/azure-mobile-apps</RepositoryUrl>
    <RootNamespace>Microsoft.AspNetCore.Datasync.Automapper</RootNamespace>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <Import Project="..\..\Shared.Build.props" />

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="11.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.AspNetCore.Datasync.Abstractions\Microsoft.AspNetCore.Datasync.Abstractions.csproj" />
  </ItemGroup>

</Project>
