<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>Microsoft.AspNetCore.Datasync.InMemory</AssemblyName>
    <Authors>Microsoft</Authors>
    <Company>Microsoft</Company>
    <Copyright>Copyright (c) 2021 Microsoft Corporation.</Copyright>
    <Description>An in-memory repository for use with the ASP.NET Core Datasync framework.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IconUrl>http://go.microsoft.com/fwlink/?LinkID=288890</IconUrl>
    <LangVersion>9.0</LangVersion>
    <LicenseUrl>https://github.com/Azure/azure-mobile-apps/blob/main/LICENSE.txt</LicenseUrl>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageId>Microsoft.AspNetCore.Datasync.InMemory</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Azure/azure-mobile-apps</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
