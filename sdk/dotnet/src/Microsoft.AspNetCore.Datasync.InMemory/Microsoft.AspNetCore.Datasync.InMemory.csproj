<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>An in-memory repository for use with the ASP.NET Core Datasync framework.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>10.0</LangVersion>
    <PackageId>Microsoft.AspNetCore.Datasync.InMemory</PackageId>
    <PackageTags>microsoft;aspnetcore;cloud;datasync;webapi</PackageTags>
    <ProductName>Microsoft.AspNetCore.Datasync.InMemory</ProductName>
    <RootNamespace>Microsoft.AspNetCore.Datasync.InMemory</RootNamespace>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <Import Project="..\..\Shared.Build.props" />

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.AspNetCore.Datasync\Microsoft.AspNetCore.Datasync.csproj" />
  </ItemGroup>
</Project>
