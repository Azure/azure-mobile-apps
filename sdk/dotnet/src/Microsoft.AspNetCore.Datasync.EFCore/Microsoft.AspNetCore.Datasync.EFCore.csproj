<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>An Entity Framework Core based repository for use with the ASP.NET Core Datasync framework.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IconUrl>http://go.microsoft.com/fwlink/?LinkID=288890</IconUrl>
    <LangVersion>10.0</LangVersion>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageId>Microsoft.AspNetCore.Datasync.EFCore</PackageId>
    <PackageTags>microsoft;aspnetcore;cloud;datasync;webapi</PackageTags>
    <ProductName>Microsoft.AspNetCore.Datasync.EFCore</ProductName>
    <ProjectUrl>https://github.com/Azure/azure-mobile-apps</ProjectUrl>
    <RepositoryUrl>https://github.com/Azure/azure-mobile-apps</RepositoryUrl>
    <RootNamespace>Microsoft.AspNetCore.Datasync.EFCore</RootNamespace>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <Import Project="..\..\Shared.Build.props" />

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.13" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.AspNetCore.Datasync\Microsoft.AspNetCore.Datasync.csproj" />
  </ItemGroup>
</Project>