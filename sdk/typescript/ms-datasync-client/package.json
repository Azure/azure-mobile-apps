{
  "name": "ms-datasync-client",
  "version": "5.0.0-alpha.1",
  "description": "A JavaScript and TypeScript SDK for connecting to a Microsoft Datasync Service (also known as Azure Mobile Apps).",
  "author": "Microsoft Corporation",
  "homepage": "https://github.com/azure/azure-mobile-apps",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azure/azure-mobile-apps/issues"
  },
  "sdk-type": "client",
  "main": "dist/index.js",
  "module": "dist-esm/src/index.js",
  "browser": {
    "./dist-esm/src/platform/configStore.js": "./dist-esm/src/platform/configStore.browser.js"
  },
  "react-native": {
    "./dist/index.js": "./dist-esm/src/index.js"
  },
  "sideEffects": false,
  "files": [
    "dist/",
    "dist-esm/src/",
    "types/latest/",
    "types/3.1/",
    "README.md",
    "../../../LICENSE.txt"
  ],
  "types": "types/latest/ms-datasync-library.d.ts",
  "typesVersions": {
    "<3.6": {
      "*": [
        "types/3.1/ms-datasync-library.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "run-s lint build:src build:api build:types",
    "build:api": "api-extractor run --local",
    "build:src": "tsc -p .",
    "build:types": "downlevel-dts types/latest/ types/3.1/",
    "clean": "rimraf dist dist-esm temp types .nyc_output coverage *.tgz *.log",
    "lint": "eslint src test --ext .ts",
    "pretest": "run-s build",
    "test": "run-s test:node test:browser test:coverage",
    "test:browser": "karma start --single-run=true",
    "test:coverage": "nyc report --reporter text",
    "test:node": "nyc --reporter none mocha --reporter progress"
  },
  "dependencies": {
    "@azure/core-auth": "^1.4.0",
    "@azure/core-client": "^1.6.1",
    "@azure/core-paging": "^1.3.0",
    "ip6addr": "^0.2.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.28.4",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-datetime": "^0.0.37",
    "@types/chai-string": "^1.4.2",
    "@types/chai-subset": "^1.3.3",
    "@types/ip6addr": "^0.2.3",
    "@types/mocha": "^9.1.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.8.0",
    "chai-string": "^1.5.0",
    "chai-subset": "^1.6.0",
    "downlevel-dts": "^0.10.0",
    "eslint": "^8.19.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-typescript": "^5.5.3",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
